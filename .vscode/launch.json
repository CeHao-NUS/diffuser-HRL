    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args":[
                "--config",
                "config.stitch.train.train_HL_varh",
                "--horizon",
                "192",
                "--n_diffusion_steps",
                "32",
                "--downsample",
                "16",
                "--min_horizon",
                "1"
            ]
        }
    ]
}


