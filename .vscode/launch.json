{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args":[
                "--config", "config.stitch.plan.plan_3_1",
                "--seg_length", "11",
                "--LL_horizon", "16",
                "--HL_horizon", "192",
                "--n_diffusion_steps", "32",
                "--downsample", "16",
                "--HL_min_horizon", "1",
                "--conditional", "True",
                "--init_pose", "(1, 3)",
                "--target", "(1, 1)",
                "--suffix", "[(1, 3)]",
                "--HL_batch_size", "8",
                "--LL_goal_reach", "True",
            ]
        }
    ]
}

