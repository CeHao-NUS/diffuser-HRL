    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args":[
                "--config",
                "config.single.plan_diff",
                "--dataset",
                "maze2d-umaze-v1",
                "--device",
                "cuda",
                "--diffusion_loadpath",
                "diffusion/ep_single/H128_T64",
                "--prefix",
                "plans/ep_single/"
            ],

        }
    ]
}

